class Demo{
	
	
	



public static void main (String[] args) {
	
	int [] array = {1,2,3,234,12312,42534,1212,424};
  big=Bignum(array);
  System.out.print(big);
   
  
	
	
}
	public static int Bignum(int[] h)   {
		
	 int big = -1;
   
	for (int i=0; i<h.length; i++)
	 {
	 	 if (big < h[i])
	 	  {
	 	  	
	 	  	big = h[i];
	 	  	
	 	  }
	 	      	
	 	  
	 	      }
	 
	 	return big;
	 }
	
}



   