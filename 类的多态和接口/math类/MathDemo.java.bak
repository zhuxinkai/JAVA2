//所有lang 下面的类都不需要导入。
class MathDemo{
	public static void main(String [] args){
		System.out.println(Math.abs(-12.5));
		System.out.println(Math.PI);
		System.out.println(Math.E);
		System.out.println(Math.round(3.25));
		System.out.println(Math.max(12,25));
		//System.out.print(Math.quart(3));
		System.out.println(Math.ceil(12.2));
		System.out.println(Math.floor(3.75));
		System.out.println(Math.sqrt(27));
		
		
	}
	
	
}

